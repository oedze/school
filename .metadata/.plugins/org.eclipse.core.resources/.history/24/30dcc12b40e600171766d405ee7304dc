void Tijdsduur::eraf(const Tijdsduur& t){
	sec -= t.sec;
}

void Tijdsduur::erbij(const Tijdsduur& t){
	sec += t.sec;
}

void Tijdsduur::maal(int multiplier){
	sec *= multiplier;
}

